@WINDOWS_TRUE@couch.app: couch.app.tpl
@WINDOWS_TRUE@	modules=`find . -name "*.erl" -exec basename {} .erl \; | tr '\n' ',' | sed "s/,$$//"`; \
@WINDOWS_TRUE@	sed -e "s|%package_name%|@package_name@|g" \
@WINDOWS_TRUE@			-e "s|%version%|@version@|g" \
@WINDOWS_TRUE@			-e "s|@modules@|$$modules|g" \
@WINDOWS_TRUE@			-e "s|%localconfdir%|../etc/couchdb|g" \
@WINDOWS_TRUE@			-e "s|@defaultini@|default.ini|g" \
@WINDOWS_TRUE@			-e "s|@localini@|local.ini|g" > \
@WINDOWS_TRUE@	$@ < $<
@WINDOWS_FALSE@couch.app: couch.app.tpl
@WINDOWS_FALSE@	modules=`{ find . -name "*.erl" -exec basename {} .erl \; | tr '\n' ','; echo ''; } | sed "s/,$$//"`; \
@WINDOWS_FALSE@	sed -e "s|%package_name%|@package_name@|g" \
@WINDOWS_FALSE@			-e "s|%version%|@version@|g" \
@WINDOWS_FALSE@			-e "s|@modules@|$$modules|g" \
@WINDOWS_FALSE@			-e "s|%localconfdir%|@localconfdir@|g" \
@WINDOWS_FALSE@			-e "s|@defaultini@|default.ini|g" \
@WINDOWS_FALSE@			-e "s|@localini@|local.ini|g" > \
@WINDOWS_FALSE@	$@ < $<
@WINDOWS_FALSE@	chmod +x $@

